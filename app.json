{
	"expo": {
		"name": "react-native-app",
		"slug": "react-native-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "",
		"scheme": "reactnativeapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"backgroundColor": "#E6F4FE"
			},
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false
		},
		"web": {
			"output": "static"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true
		}
	}
}
